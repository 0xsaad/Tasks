<!DOCTYPE html>
<html>
<head> 
    <title>Document</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>
<body>
    <div class="container">
        <table class="table table-border table-bordered text-center shadow">
            <tr>
                 <th>singer id</th>
                 <th>singer name</th>
                 <th>singer song</th>
                 <th>singer band</th>
                 <th>singer delete</th>
                 <th>singer update</th>
            </tr>
            <% singers.forEach(function(singer){  %>
                <tr>
                     <td><%=singer._id %></td>
                     <td><%=singer.name %></td>
                     <td><%=singer.song %></td>
                     <td><%=singer.band %></td>
                     <td><a href="/delete/<%=singer._id %>" class="btn btn-danger">delete</a></td>
                     <td><a href="/edit/<%=singer._id %>" class="btn btn-success">edit</a></td>
                </tr>
                <% }) %>
        </table>
    </div>
    
</body>
</html>